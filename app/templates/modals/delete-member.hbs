{{#modal-dialog title=(t 'member.deleteQuestion') ok='delete' okText=(t 'delete') okClass='btn-danger' close='closeModal' closeText=(t 'cancel')}}
    <h2>{{model.fullname}}</h2>
    <p>
        {{model.role}}
    </p>
    <p>
        {{model.description}}
    </p>
    {{#if model.photo}}
        <p>
            <img src={{model.photo.smallUrl}} class="img-responsive center-block" />
        </p>
    {{/if}}
{{/modal-dialog}}
